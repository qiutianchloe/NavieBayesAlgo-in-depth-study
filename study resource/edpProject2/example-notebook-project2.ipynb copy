    "###5b code\n",
    "\n",
    "#create the dataFrame that store 2 features  'Energy, with dietary fibre (kJ)' and 'Protein (g)'\n",
    "Energy = food['Energy, with dietary fibre (kJ)']\n",
    "Protein = food['Protein (g)']\n",
    "DF = pd.DataFrame({'Energy':Energy, 'Protein':Protein})\n",
    "\n",
    "# create a dictionary to store all the bin-width and corresponding MI value\n",
    "MI = {}\n",
    "\n",
    "\n",
    "#find the MI for bin-width equal to 2\n",
    "result = mutual_info(DF, 2)\n",
    "#put the into the MI dictionary \n",
    "MI[2] = result\n",
    "\n",
    "# set the bin-width 10,20,30,40,....200\n",
    "for l in range(10, 200, 10):\n",
    "    #find the MI for the certain bin-width\n",
    "    result = mutual_info(DF, l)\n",
    "    #put the corresponding MI value into the MI dictionary \n",
    "    MI[l] = result\n",
    "    \n",
    "#plot the MI out\n",
    "plt.figure()\n",
    "plt.plot(list(MI.keys()), list(MI.values()))\n",
    "plt.xlabel(\"Number of bins\")\n",
    "plt.ylabel(\"MI\")\n",
    "plt.show()    \n",
    "    "
   ]
